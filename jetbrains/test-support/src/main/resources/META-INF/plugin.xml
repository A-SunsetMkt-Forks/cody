<idea-plugin>
    <vendor>Sourcegraph</vendor>
    <id>com.sourcegraph.jetbrains.testing</id>
    <name>Cody Test Support</name>
    <depends>com.sourcegraph.jetbrains</depends>
    <extensions defaultExtensionNs="com.intellij">
        <postStartupActivity implementation="com.sourcegraph.jetbrains.testing.DebugServerStartupActivity" />
        <postStartupActivity implementation="com.sourcegraph.jetbrains.testing.ChromeDevToolsSnitch" />
        <!-- To debug the Playwright <~~~> Chrome DevTools interaction with mitmproxy, etc. use this to expose devtools directly. -->
        <!-- <registryKey defaultValue="9226" description="Override this internal key" key="ide.browser.jcef.debug.port" /> -->
    </extensions>
    <extensions defaultExtensionNs="com.sourcegraph.jetbrains">
        <testSupport.webviewSink implementation="com.sourcegraph.jetbrains.testing.WebviewAutomation" />
    </extensions>
</idea-plugin>
